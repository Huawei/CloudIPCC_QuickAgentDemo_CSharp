<Window x:Class="QuickAgent.Src.View.TextChat.TC_Common"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="{DynamicResource textmsg_innercall}" Height="110" Width="250" ResizeMode="NoResize" ShowInTaskbar="False" WindowStartupLocation="CenterScreen" Topmost="True">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition/>
            <RowDefinition Height="35"/>
        </Grid.RowDefinitions>
        <Label Content="{DynamicResource jobNum}" VerticalAlignment="Center" HorizontalAlignment="Left" Width="50" Margin="20,0,0,0"/>
        <TextBox Name="tb_workNo" VerticalAlignment="Center" HorizontalAlignment="Left" Margin="60,0,0,0" Width="150"/>

        <Button Name="btn_ok" VerticalAlignment="Top" HorizontalAlignment="Left" Margin="50,0,0,0" Content="{DynamicResource ok}" Width="60"  Grid.Row="1" Click="btn_ok_Click" />
        <Button Name="btn_cancel" VerticalAlignment="Top" HorizontalAlignment="Left" Margin="140,0,0,0" Content="{DynamicResource btn_cancel}" Width="60"  Grid.Row="1" Click="btn_cancel_Click"/>
    </Grid>
</Window>
