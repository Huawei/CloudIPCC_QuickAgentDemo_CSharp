    <!--标注背景变换-->
    <Style x:Key="CalledStateStyle" TargetType="StackPanel">
        <!--<Setter Property="HorizontalAlignment" Value="Left"></Setter>
        <Setter Property="Margin" Value="1,0,0,27"></Setter>-->
        <Setter Property="Cursor" Value="Hand"></Setter>
        <!--鼠标移入移出-->
        <Style.Triggers>
            <Trigger Property="IsMouseOver" Value="True">
                <Setter Property="Background"  Value="{StaticResource StateSetMouseOver}"/>
            </Trigger>
        </Style.Triggers>
    </Style>
    <!--应用共享背景变换-->
    <Style x:Key="AppStyle" TargetType="Grid">
        <Setter Property="Background" Value="WhiteSmoke"></Setter>
        <!--鼠标移入移出-->
        <Style.Triggers>
            <Trigger Property="IsMouseOver" Value="True">
                <Setter Property="Background"  Value="#fffcea"/>
            </Trigger>
        </Style.Triggers>
    </Style>

    <!--标注背景变换-->
    <Style x:Key="AnnotationStyle" TargetType="StackPanel">
        <Setter Property="Height" Value="22"></Setter>
        <Setter Property="Width" Value="140"></Setter>
        <Setter Property="Margin" Value="3"></Setter>
        <!--鼠标移入移出-->
        <Style.Triggers>
            <Trigger Property="IsMouseOver" Value="True">
                <Setter Property="Background"  Value="AliceBlue"/>
            </Trigger>
        </Style.Triggers> 
    </Style>
   
    <!--设置TabItem样式-->
    <Style x:Key="LisTabItemStyle" TargetType="{x:Type TabItem}">
        <Setter Property="HorizontalContentAlignment" Value="Stretch"/>
        <Setter Property="VerticalContentAlignment" Value="Stretch"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type TabItem}">
                    <Grid SnapsToDevicePixels="true">
                        <StackPanel Orientation="Horizontal" Margin="1,0,4,0">
                            <TextBlock x:Name="tbLeft"  Width="4" Height="26"  Background="{StaticResource TabItemLeft}"/>
                            <TextBlock x:Name="Bd"  Height="26" TextAlignment="Center" Foreground="#333333" Padding="5,5.5,5,0" Background="{StaticResource TabItemCenter}" Text="{TemplateBinding Header}"/>
                            <TextBlock x:Name="tbRight" Width="4" Height="26" Background="{StaticResource TabItemRight}"/>
                        </StackPanel>
                        
                    </Grid>
                    <ControlTemplate.Triggers>
                       
                        <!--选择后颜色-->
                        <Trigger Property="IsSelected" Value="true">
                            <Setter Property="Background" TargetName="tbLeft" Value="{StaticResource ItemSelectedLeft}"></Setter>
                            <Setter Property="Background" TargetName="Bd"  Value="{StaticResource ItemSelectedCenter}"></Setter>
                            <Setter Property="Foreground" TargetName="Bd"  Value="#004161"></Setter>
                            <Setter Property="Background" TargetName="tbRight" Value="{StaticResource ItemSelectedRight}"></Setter>
                        </Trigger>
                        <Trigger Property="IsEnabled" Value="false">
                            <Setter Property="Background" TargetName="tbLeft" Value="{StaticResource ItemIsEnabledLeft}"></Setter>
                            <Setter Property="Background" TargetName="Bd" Value="{StaticResource ItemIsEnabledCenter}"></Setter>
                            <Setter Property="Foreground" TargetName="Bd"  Value="#a4a4a4"></Setter>
                            <Setter Property="Background" TargetName="tbRight" Value="{StaticResource ItemIsEnabledRight}"></Setter>
                        </Trigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

    <!--设置ToggleButton选中项的颜色-->
    <Style x:Key="ToggleButtonStyle" TargetType="{x:Type ToggleButton}">
        <Style.BasedOn>
            <Style TargetType="{x:Type ToggleButton}">
                <Setter Property="Template">
                    <Setter.Value>
                        <ControlTemplate TargetType="{x:Type ToggleButton}">
                            <Border Name="border" Background="{StaticResource InputBackgroun}">
                                <ContentPresenter HorizontalAlignment="Center" VerticalAlignment="Center" />
                            </Border>
                            <ControlTemplate.Triggers>
                                <Trigger Property="IsChecked" Value="True">
                                    <Setter Property="Background" TargetName="border" Value="{StaticResource LoginBackgroun}"/>
                                </Trigger>
                            </ControlTemplate.Triggers>
                        </ControlTemplate>
                    </Setter.Value>
                </Setter>
            </Style>
        </Style.BasedOn>
    </Style>

    <!--设置Expander样式-->
    <Style x:Key="ExpanderStyle" TargetType="{x:Type Expander}">
        <Setter Property="BorderBrush" Value="White"/>
        <Setter Property="BorderThickness" Value="0"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type Expander}">
                    <Border SnapsToDevicePixels="true" BorderBrush="{TemplateBinding BorderBrush}" BorderThickness="1,1,1,1">
                        <Grid>
                            <DockPanel>
                                <ToggleButton Name="HeaderSite" Content="{TemplateBinding Header}" Height="22" Style="{StaticResource ToggleButtonStyle}" IsChecked="{Binding Path=IsExpanded, Mode=TwoWay, RelativeSource={RelativeSource TemplatedParent}}" DockPanel.Dock="Top"/>
                                <Border Name="border" Visibility="Collapsed">
                                    <ContentPresenter Name="ExpandSite" DockPanel.Dock="Bottom"/>
                                </Border>
                            </DockPanel>
                        </Grid>
                    </Border>
                    <ControlTemplate.Triggers>
                        <Trigger Property="IsExpanded" Value="true">
                            <Setter Property="Visibility" TargetName="border" Value="Visible"/>
                        </Trigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

    <!--设置工号输入框的背景-->
    <Style x:Key="TextBoxStyle" TargetType="TextBox">
        <Style.Setters>
            <!--取消默认边框-->
            <Setter Property="BorderThickness" Value="0"/>
            <!--输入内容居中-->
            <Setter Property="VerticalContentAlignment" Value="Center"/>
            <Setter Property="Background" Value="{StaticResource InputBackgroun}"/>
        </Style.Setters>
    </Style>

    <!--设置密码输入框的背景-->
    <Style x:Key="PasswordBoxStyle" TargetType="PasswordBox">
        <Style.Setters>
            <!--取消默认边框-->
            <Setter Property="BorderThickness" Value="0"/>
            <!--输入内容居中-->
            <Setter Property="VerticalContentAlignment" Value="Center"/>
            <Setter Property="Background" Value="{StaticResource InputBackgroun}"/>
        </Style.Setters>
    </Style>

    <!--设置登录按钮的背景-->
    <Style x:Key="LoginButtonStyle" TargetType="Button">
        <Style.Setters>
            <!--取消默认边框-->
            <Setter Property="BorderThickness" Value="0"/>
            <!--输入内容居中-->
            <Setter Property="VerticalContentAlignment" Value="Center"/>
            <Setter Property="Background" Value="{StaticResource LoginBackgroun}"/>
            <Setter Property="Template">
                <Setter.Value>
                    <ControlTemplate TargetType="{x:Type Button}">
                        <!--初始背景-->
                        <Border Name="back" BorderThickness="0" Background="{StaticResource LoginBackgroun}">
                            <TextBlock Text="{TemplateBinding Button.Content}"  TextAlignment="Center" Padding="{TemplateBinding Button.Padding}"></TextBlock>
                        </Border>
                        <ControlTemplate.Triggers>
                            <!--按钮按下弹起-->
                            <Trigger Property="IsPressed" Value="True">
                                <Setter Property="Background" TargetName="back" Value="{StaticResource LoginPreeground}"></Setter>
                            </Trigger>
                        </ControlTemplate.Triggers>
                    </ControlTemplate>
                </Setter.Value>
            </Setter>
            
        </Style.Setters>
    </Style>

    <!--设置ListBox样式-->
    <Style x:Key="ListBoxStyle" TargetType="{x:Type ListBox}">
        <Setter Property="Background" Value="{DynamicResource {x:Static SystemColors.WindowBrushKey}}"/>
        <Setter Property="BorderThickness" Value="1"/>
        <Setter Property="Foreground" Value="{DynamicResource {x:Static SystemColors.ControlTextBrushKey}}"/>
        <Setter Property="ScrollViewer.HorizontalScrollBarVisibility" Value="Auto"/>
        <Setter Property="ScrollViewer.VerticalScrollBarVisibility" Value="Auto"/>
        <Setter Property="ScrollViewer.CanContentScroll" Value="true"/>
        <Setter Property="VerticalContentAlignment" Value="Center"/>
        <Setter Property="ItemTemplate">
            <Setter.Value>
                <DataTemplate>
                    <!-- 这里设置自己定义的控件和显示-->
                    <Grid>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="100"/>
                            <ColumnDefinition Width="60"/>
                        </Grid.ColumnDefinitions>
                        <TextBlock Grid.Column="0" Text="{Binding cId}" />
                        <TextBlock Grid.Column="1" Text="{Binding cName}" />
                    </Grid>
                </DataTemplate>
            </Setter.Value>
        </Setter>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type ListBox}">
                    <Border x:Name="Bd" BorderBrush="{TemplateBinding BorderBrush}" BorderThickness="{TemplateBinding BorderThickness}" Background="{TemplateBinding Background}" Padding="1" SnapsToDevicePixels="true">
                        <ItemsPresenter SnapsToDevicePixels="{TemplateBinding SnapsToDevicePixels}"/>
                    </Border>
                    <ControlTemplate.Triggers>
                        <Trigger Property="IsEnabled" Value="false">
                            <Setter Property="Background" TargetName="Bd" Value="{DynamicResource {x:Static SystemColors.ControlBrushKey}}"/>
                        </Trigger>
                        <Trigger Property="IsGrouping" Value="true">
                            <Setter Property="ScrollViewer.CanContentScroll" Value="false"/>
                        </Trigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

    <!--设置ListBox选中项的颜色-->
    <Style x:Key="ListBoxItemStyle" TargetType="ListBoxItem">
        <Setter Property="Height" Value="20"/>
        <Style.Resources>
            <!--SelectedItem with focus-->
            <SolidColorBrush x:Key="{x:Static SystemColors.HighlightBrushKey}" Color="#BCE8FF" />
            <!--SelectedItem without focus-->
            <SolidColorBrush x:Key="{x:Static SystemColors.ControlBrushKey}" Color="#BCE8FF" />
        </Style.Resources>
    </Style>


