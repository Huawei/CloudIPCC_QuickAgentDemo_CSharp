    <!--硬件信息列表背景色-->
    <Style x:Key="SpContenStyle" TargetType="{x:Type StackPanel}">
        <Setter Property="Background" Value="{StaticResource HardwareInfoContentBg}"></Setter>
    </Style>

    <!--ComboBox背景图片-->
    <Style x:Key="ComboBoxStyle" TargetType="{x:Type ComboBox}">
        <Setter Property="Background" Value="{StaticResource ComboBoxBackGround}"></Setter>
    </Style>
</ResourceDictionary>